<form accept-charset="UTF-8" action="/users/create" method="post">
  <label for="username">Username</label>
  <input type="text" id="user_username" name="user[username]"><br><br>
  <label for="email">Email:</label>
  <input type="email" id="user_email" name="user[email]"><br><br>
  <label for="password">Password:</label>
  <input type="password" id="user_password" name="user[password]"><br><br>
  <input type="submit" value="Submit">
</form>